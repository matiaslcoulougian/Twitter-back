{
  "name": "emergencias-api",
  "version": "1.0.0",
  "description": "Emergencias API",
  "scripts": {
    "lint": "eslint --ext .ts --format=pretty src/**/*.ts",
    "test:unit": "jest --config=./jest.config.js",
    "test:integration": "jest --runInBand --config=./jest.config.integration.js",
    "build": "tsc -p tsconfig.app.json",
    "start": "node -r ./scripts/bootstrap.js build/server.js",
    "dev": "ts-node-dev -r ./scripts/bootstrap.js --rs src/server",
    "db": "knex --knexfile src/db/knexfile.ts",
    "db migrate:make": "knex migrate:make",
    "db:rollback": "npm run db migrate:rollback --all",
    "db:migrate": "npm run db migrate:latest",
    "db:seed": "npm run db seed:run",
    "db:list": "npm run db migrate:list",
    "db:reset": "ts-node -r ./scripts/bootstrap.js ./scripts/reset-db.ts"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.11.6",
    "@types/node-fetch": "^2.5.10",
    "@types/pg-connection-string": "^2.0.0",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "jest": "^26.5.2",
    "knex-cleaner": "^1.3.1",
    "pg-connection-string": "^2.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node-dev": "^1.0.0-pre.63"
  },
  "dependencies": {
    "argon2": "^0.27.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "jwks-rsa": "^1.11.0",
    "jwt-decode": "^3.1.2",
    "knex": "^0.21.21",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nanoid": "^3.1.22",
    "nock": "^13.0.7",
    "node-fetch": "^2.6.1",
    "objection": "^2.2.10",
    "pg": "^8.4.1",
    "request-promise": "^4.2.6",
    "swagger-jsdoc": "6.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zod": "^1.11.13"
  },
  "engines": {
    "node": "16.8.0",
    "npm": "7.21.0"
  }
}
